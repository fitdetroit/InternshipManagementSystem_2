<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
    "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="companyUserName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter a Company User Name</message>
		</field-validator>
	</field>
	<field name="companyName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter Company Name</message>
		</field-validator>
	</field>
	<field name="companyTelephone">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter Company Telephone No</message>
		</field-validator>
	</field>
	<field name="contactPerson">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter Contact Persion Name </message>
		</field-validator>
	</field>
	<field name="companyAddress">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter Company Address</message>
		</field-validator>
	</field>
	<field name="companyWeb">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter Company Web Url</message>
		</field-validator>
	</field>
	<field name="noOfVacancies">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please enter No Of VAcancies</message>
		</field-validator>
	</field>


	<field name="password">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Password is required</message>
		</field-validator>
	</field>
	



</validators>